<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.for_exam1">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.For_exam1"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".MenusActivity"
            android:exported="false" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".HistoryActivity" />
        <activity android:name=".DriveTimerActivity" />
        <activity android:name=".WaitTimeActivity" />
        <activity android:name=".StartScreenActivity" />
        <activity android:name=".MainActivityRegister" />
        <activity
            android:name=".LaunchScreen"
            android:theme="@style/SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>